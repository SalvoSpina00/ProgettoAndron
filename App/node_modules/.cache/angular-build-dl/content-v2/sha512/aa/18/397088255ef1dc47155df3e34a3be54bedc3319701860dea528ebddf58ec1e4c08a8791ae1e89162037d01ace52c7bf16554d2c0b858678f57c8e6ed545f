a(){\n    if (this.cartService.getCart().length == 0) {\n      this.allert(\"Il carrello e' vuoto\")\n      return\n    }\n    const alert = await this.alertCtrl.create({\n      header: \"Attenzione\",\n      message: \"Vuoi proseguire con l'ordine?\",\n      buttons: [\n        {\n          text: 'No',\n          role: 'cancel',\n          \n        },\n        { text: 'Si',\n          handler: () => { this.checkout()} \n        }\n      ]\n    })\n    await alert.present();\n  }\n  async checkout() {\n    //! Controllo se il saldo e' sufficiente\n    this.apiService.getBudget().then(retApiService => {\n      return retApiService.subscribe(async response => {\n        let saldo = response[\"_msg\"].budget;\n        if (saldo < this.getTotal()) {\n          this.allert(\"Saldo insufficiente per effettuare l'operazione\");\n        }//! se sufficiente effettuo l'operazione\n        else {\n          this.apiService.newTransazione(this.cart).then(retApiService => {\n            retApiService.subscribe(async response => {\n              let alert = await this.alertCtrl.create({\n                header: 'Grazie per il tuo ordine!',\n                message: 'Spero che usufrirai ancora dei nostri servizi',\n                buttons: ['OK']\n              });\n              alert.present().then(() => {\n                this.cart = [];\n                this.cartService.resetCart();\n                this.modalCtrl.dismiss();\n              });\n            },\n              errore => {\n                this.errore = errore[\"error\"][\"_msg\"];\n                this.allert(this.errore);\n              });\n          });\n          \n        };\n      })\n    })\n  }\n}","export default \"h1, h3 {\\n  text-transform: capitalize;\\n}\\n\\nion-text {\\n  font-size: 12px;\\n}\\n\\n#idListMenu ion-icon {\\n  padding-right: 17px;\\n  color: #1d273d;\\n}\\n\\nion-item {\\n  color: #1d273d;\\n}\\n/*# sourceMappingURL=data:application/json;base64, */\";","import { enableProdMode } fr