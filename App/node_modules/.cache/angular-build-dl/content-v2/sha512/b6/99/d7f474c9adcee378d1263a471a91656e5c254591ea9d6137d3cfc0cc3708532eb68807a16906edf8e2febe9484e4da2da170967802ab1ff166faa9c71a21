                header: "Attenzione",
                message: "Vuoi davvero eliminare questo serivizio?",
                buttons: [
                    { text: 'No', },
                    {
                        text: 'Si',
                        handler: () => {
                            this.delete(idService);
                        }
                    }
                ]
            });
            yield alert.present();
        });
    }
    openNewService() {
        this.navCtrl.navigateForward("/nuovo-servizio");
    }
