 {}\n","export class Servizio {\n    nome: string;  \n    descrizione: string;\n    prezzo: number;\n}","import { Component, OnInit } from '@angular/core';\nimport { ApiService } from 'src/app/services/api-service.service';\nimport { NavController } from '@ionic/angular';\nimport { Storage } from '@ionic/storage';\nimport { Servizio } from 'src/app/models/servizio';\n\n@Component({\n  selector: 'app-nuovo-servizio',\n  templateUrl: './nuovo-servizio.page.html',\n  styleUrls: ['./nuovo-servizio.page.scss'],\n})\nexport class NuovoServizioPage implements OnInit {\n  servizio : Servizio;\n  nome: string;\n  descrizione: string;\n \n  errore: string;\n\n  constructor(private apiService: ApiService, private navCtrl: NavController, private storage: Storage) {\n\n   }\n\n  ngOnInit() {\n  }\n\n  registerService(){\n    this.errore=\"\";\n    this.servizio = new Servizio()\n    this.servizio.nome=this.nome;\n    this.servizio.descrizione=this.descrizione\n    this.servizio.prezzo=10;//! Prezzo di default\n\n\n    if(this.nome && this.descrizione){\n      if (this.nome.trim() == '' || this.descrizione.trim() == '') {\n        this.errore = \"Compila adeguatamente i campi\"\n        return\n      }\n      this.apiService.newServizio(this.servizio).then(retApiService => {\n        retApiService.subscribe( response => {\n          console.info(response[\"_msg\"]);\n          this.navCtrl.navigateForward(\"/servizi\");\n        });\n      })\n    }\n    else{\n      this.errore=\"Tutti i campi devono essere compilati.\"\n      console.log(this.errore);\n    } \n  }\n  \n}\n","export default \"p {\\n  color: red;\\n  text-align: center;\\n}\\n\\nion-item {\\n  color: #457c8d;\\n  --highlight-color-invalid: red;\\n  --highlight-color-valid: yellow;\\n}\\n\\nion-item:hover {\\n  background-color: #404f79;\\n}\\n/*# sourceMappingURL=data:application/json;base64, */\";","import { NgModule } from '@angular/core';\nimport { CommonModule } from '@angular/common';\nimport { FormsModule }