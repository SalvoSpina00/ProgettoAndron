// Modals



// Call notifications test by Popover and Custom Component.
//import { NotificationsComponent } from './../../components/notifications/notifications.component';
let CompraPage = class CompraPage {
    constructor(navCtrl, menuCtrl, popoverCtrl, alertCtrl, modalCtrl, toastCtrl, cartService, router, apiService) {
        this.navCtrl = navCtrl;
        this.menuCtrl = menuCtrl;
        this.popoverCtrl = popoverCtrl;
        this.alertCtrl = alertCtrl;
        this.modalCtrl = modalCtrl;
        this.toastCtrl = toastCtrl;
        this.cartService = cartService;
        this.router = router;
        this.apiService = apiService;
        this.cartItemCount = 0;
        this.filtro = false;
    }
    ngOnInit() {
        this.cart = this.cartService.getCart();
        this.cartItemCount = 0;
    }
    ionViewWillEnter() {
        this.page = 0;
        this.psize = 5;
        this.initializeItems();
        this.menuCtrl.enable(true);
    }
    //!Inizializzo la pagina
    initializeItems() {
        this.apiService.getAllBeni(this.page, this.psize, this.filtro, this.termine, this.lowerPrice, this.upperPrice).then(retApiService => {
            retApiService.subscribe(response => {
                console.info(response);
                this.prodotti = response["_msg"];
            }, errore => {
                console.info(errore["error"]);
            });
        });
    }
    // start cart
    addToCart(product) {
        this.cartService.addProduct(product);
        this.cartItemCount += 1;
    }
    feedback(id, tipo, nome) {
        this.router.navigate(["/infofeedback", { "id": id, "tipo": tipo, "nome": nome }]);
    }
    openCart() {
        return Object(tslib__WEBPACK_IMPORTED_MODULE_0__["__awaiter"])(this, void 0, void 0, function* () {
            const modal = yield this.modalCtrl.create({
                component: _cart_modal_cart_modal_page__WEBPACK_IMPORTED_MODULE_6__["CartModalPage"],
            });
            modal.onDidDismiss().then((data) => {
                this.cartItemCount = this.cartService.getCartItemCount();
                //console.info(JSON.stringify({cart: this.cart}));
                console.info("close modal");
            });
            return yield modal.present();
        });
    }
    searchFilter() {
        return Object(tslib__WEBPACK_IMPORTED_MODULE_0__["__awaiter"])(this, void 0, void 0, function* () {
            if ((this.upperPrice == undefined) && (this.lowerPrice == undefined)) {
                this.upperPrice = 500;
                this.lowerPrice = 0;
            }
            const modal = yield this.modalCtrl.create({
                component: _pages_modal_search_filter_search_filter_page__WEBPACK_IMPORTED_MODULE_7__["SearchFilterPage"],
                componentProps: {
                    upperPrice: this.upperPrice,
                    lowerPrice: this.lowerPrice
                }
            });
            modal.onDidDismiss().then((data) => {
                this.upperPrice = data.data["upper"];
                this.lowerPrice = data.data["lower"];
                this.filtro = true;
                this.initializeItems();
            });
            return yield modal.present();
        });
    }
    rimuoviFiltri() {
        this.page = 0;
        this.filtro = false;
        this.termine = '';
        this.lowerPrice = undefined;
        this.upperPrice = undefined;
        this.initializeItems();
    }
    filter(event) {
        let val = event.target.value;
        if (val.trim() != '') {
            this.page = 0;
            this.filtro = true;
            this.termine = val;
        }
        else {
            this.termine = '';
        }
        this.initializeItems();
    }
    clickItem(product) {
        this.router.navigate(["/descrizione-prodotto", { "product": JSON.stringify(product) }]);
    }
    loadmore(event) {
        this.page++;
        this.apiService.getAllBeni(this.page, this.psize, this.filtro, this.termine, this.lowerPrice, this.upperPrice).then(retApiService => {
            retApiService.subscribe(results => {
                for (const t of results["_msg"]) {
                    this.prodotti.push(t);
                }
                ;
                event.target.complete();
            });
        });
    }
};
CompraPage.ctorParameters = () => [
