duct.idProdotto) {\n        p.quantita += 1;\n        this.cartItemCount = this.cartItemCount + 1;\n      }\n    }\n  }\n\n  resetCart(){\n    this.cart=[]\n    this.cartItemCount=1;\n  }\n\n  removeProduct(product){\n    this.cartItemCount -= product.quantita;\n    for (let p of this.cart) {\n      if (p.idProdotto == product.idProdotto) {\n        p.quantita=1;\n        this.cart.splice(this.cart.indexOf(p), 1);\n      }\n    }\n  }\n\n}","export default \"<ion-header>\\n  <ion-toolbar>\\n    <ion-title>Carrello</ion-title>\\n    <ion-button (click)=\\\"close()\\\" fill=\\\"clear\\\" slot=\\\"start\\\">\\n      <ion-icon name=\\\"close\\\" slot=\\\"start\\\"></ion-icon>\\n    </ion-button>\\n  </ion-toolbar>\\n</ion-header>\\n\\n<ion-content>\\n    \\n    <ion-list>\\n      <ion-item *ngFor=\\\"let p of this.cart\\\">\\n        <ion-grid>\\n          <ion-row>\\n            <ion-col>\\n              <b>{{ p.nomeProdotto }}</b>\\n            </ion-col>\\n            <ion-col>\\n              <ion-button color=\\\"warning\\\" fill=\\\"clear\\\" (click)=\\\"decreaseCartItem(p)\\\">\\n                <ion-icon id=\\\"idbutton\\\" ios=\\\"ios-remove-circle\\\" md=\\\"md-remove-circle\\\"></ion-icon>\\n              </ion-button>\\n\\n              <ion-text style=\\\"color:skyblue\\\"class=\\\"ion-text-md-justify\\\">{{ p.quantita }}</ion-text>\\n\\n              <ion-button color=\\\"warning\\\" fill=\\\"clear\\\" (click)=\\\"increaseCartItem(p)\\\">\\n                <ion-icon id=\\\"idbutton\\\" ios=\\\"ios-add-circle\\\" md=\\\"md-add-circle\\\"></ion-icon>\\n              </ion-button>\\n            </ion-col>\\n          </ion-row>\\n          <ion-row>\\n            <ion-col style=\\\"color: skyblue;\\\">\\n              <ion-text>{{ p.quantita * p.prezzo}} TOKEN</ion-text>\\n            </ion-col>\\n            <ion-col class=\\\"ion-text-end\\\">\\n              <ion-button fill=\\\"clear\\\" (click)=\\\"removeCartItem(p)\\\">\\n                <ion-icon ios=\