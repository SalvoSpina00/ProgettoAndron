 && this.citta){\n      if (this.msg.trim() == '' || this.nomeEnte.trim() == '' || this.telefonoEnte.trim() == '' || this.mailEnte.trim() == '' || this.regione.trim() == '' || this.citta.trim() == '') {\n        this.errore = \"Compila adeguatamente tutti i campi\"\n        return\n      }\n      \n      this.servizio = new ServizioCollettivita(this.msg,this.date,this.nomeEnte,this.telefonoEnte,this.mailEnte,this.regione,this.citta);\n      this.api.newServizioCollettivita(this.servizio).then(retApiService => {\n        retApiService.subscribe(async response => {\n          console.log(response[\"_msg\"])\n          const alert = await this.alertCtrl.create({\n            header: \"Completato\",\n            message: \"Richiesta inviata correttamente\",\n            buttons: [\n              {\n                text: 'Ok',\n                role: 'cancel',\n\n              }]\n          })\n          await alert.present();\n        })\n      });\n    }\n    else{\n      this.errore=\"Compilare tutti i campi\"\n    }\n  }\n\n}\n","export default \"<ion-header>\\n  <ion-toolbar>\\n    <ion-title>Collettivita</ion-title>\\n    <ion-buttons slot=\\\"start\\\">\\n      <ion-menu-button autoHide=\\\"false\\\"></ion-menu-button>\\n    </ion-buttons>\\n  </ion-toolbar>\\n</ion-header>\\n\\n\\n<ion-content>\\n  <ion-card>\\n    <ion-card-header>\\n      <ion-card-title>\\n        <div class=\\\"ion-text-center\\\">\\n          Benvenuto!\\n        </div>\\n      </ion-card-title>\\n    </ion-card-header>\\n    <ion-card-content>\\n      Qui potrai inserire eventuali servizi offerti alla collettivita'. Compilare tutti i campi per mandare la richiesta, una volta verificata verranno caricati dei token sul tuo account.\\n    </ion-card-content>\\n  </ion-card>\\n\\n\\n  <ion-list>\\n    <ion-item>\\n      <ion-textarea placeholder=\\\"Descrivi il tuo servizio offerto...\\\"   maxlength=\\\"150\\\"[(ngModel)]=\\\"msg\\\"></ion-textarea>\\n    </ion-item>\\n    <ion-item>\\n      <i