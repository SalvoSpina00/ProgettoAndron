ng;\n  via: string;\n  cap: string;\n  requestRegisterUtente: RequestRegisterUtente;\n  errore: string;\n\n  constructor(private apiService: ApiService, private navCtrl: NavController, private alertController: AlertController) { }\n\n  ngOnInit() {\n  }\n\n  register(){\n    this.errore = \"\";\n    if (this.username && this.password && this.checkPassword && this.nome && this.cognome && this.date && this.cf && this.telefono && this.mail && this.stato && this.regione && this.citta && this.via && this.cap){\n      this.requestRegisterUtente = new RequestRegisterUtente(this.username, this.password, this.checkPassword, this.nome, this.cognome, this.date, this.cf,this.telefono, this.mail, this.stato, this.regione, this.citta, this.via, this.cap);\n      //console.info(this.requestRegisterUtente.date);\n      this.apiService.registerUtente(this.requestRegisterUtente).subscribe(response => {\n        //* Alert che informa la corretta registrazione\n        this.alertController.create({\n          header: \"Registrazione avvenuta con successo\",\n          buttons: [\n          {text: 'OK',\n          role: 'cancel',//! con role cancel anche se non viene premuto viene eseguito l'handler\n            handler: () => {\n              this.navCtrl.navigateForward(\"/login\");\n            }\n          }]\n        }).then(alert=> alert.present());       \n      },\n      errore=>{\n        this.errore = errore[\"error\"][\"_msg\"];\n      });\n    }\n    else{\n      this.errore = \"Tutti i campi devono essere complilati.\";\n    }\n    \n  }\n}\n"]}