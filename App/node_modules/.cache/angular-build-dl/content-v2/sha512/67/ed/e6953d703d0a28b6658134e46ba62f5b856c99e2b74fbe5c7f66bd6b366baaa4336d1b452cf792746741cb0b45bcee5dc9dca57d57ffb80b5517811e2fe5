let NuovoServizioPage = class NuovoServizioPage {
    constructor(apiService, navCtrl) {
        this.apiService = apiService;
        this.navCtrl = navCtrl;
    }
    ngOnInit() { }
    registerService() {
        var _a, _b;
        this.errore = "";
        this.nome = (_a = this.nome) !== null && _a !== void 0 ? _a : "";
        this.descrizione = (_b = this.descrizione) !== null && _b !== void 0 ? _b : "";
        if (this.nome.trim() === "" || this.descrizione.trim() === "") {
            this.errore = "Compila adeguatamente i campi";
            return;
        }
        if (isNaN(this.preavviso) || this.preavviso < 0 || this.preavviso > 7) {
            this.errore = "Il numero di giorni di preavviso dev'essere un numero positivo (massimo 7 giorni)";
            return;
        }
        if (isNaN(this.ore) || this.ore < 0 || this.ore > 5) {
            this.errore = "Il numero di ore massime dedicabili dev'essere un numero positivo (massimo 5 ore)";
            return;
        }
        let servizio = {
            "nome": this.nome,
            "descrizione": this.descrizione,
            "preavviso": this.preavviso,
            "ore": this.ore
        };
        this.apiService.newServizio(servizio).then(retApiService => {
            retApiService.subscribe({
                next: (result) => {
                    this.apiService.simplyAlert(() => {
                        this.navCtrl.navigateForward("/servizi");
                    }, "Operazione effettuata", "Il servizio Ã¨ stato inviato correttamente, una volta accettato gli altri utenti potranno richiederlo");
                },
                error: (error) => {
                    this.apiService.simplyAlert(() => {
                        this.navCtrl.navigateForward("/servizi");
                    });
                }
            });
        });
    }
};
