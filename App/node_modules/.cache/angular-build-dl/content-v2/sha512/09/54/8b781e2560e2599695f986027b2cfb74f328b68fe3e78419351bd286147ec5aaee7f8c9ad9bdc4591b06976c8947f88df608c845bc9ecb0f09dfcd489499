App: string;\n    private _accountType: string;\n    private foto: any;\n\n    \n    constructor(username: string, password:string, checkPassword:string, nome: string, cognome:string, date:Date,cf:string,telefono:string,mail:string,stato:string, regione:string,citta:string,via:string,cap:string, foto:any){\n        this._username = username;\n        this._password = password;\n        this._checkPassword = checkPassword;\n        this._nome = nome;\n        this._cognome = cognome;\n        this._date = date;\n        this._cf = cf;\n        this._telefono = telefono;\n        this._mail = mail;\n        this._stato = stato;\n        this._regione = regione;\n        this._citta = citta;\n        this._via = via;\n        this._cap = cap;\n        this._keyApp = \"keyAppSecretPassUtente\";\n        this._accountType= \"utente\";\n        this.foto=foto\n    }\n}\n","export default \"p {\\n  color: red;\\n  text-align: center;\\n}\\n\\nion-item {\\n  color: #181313;\\n  --highlight-color-invalid: red;\\n  --highlight-color-valid: rgb(24, 19, 19);\\n}\\n\\nion-item:hover {\\n  background-color: #262847;\\n}\\n\\n#idDiv ion-input {\\n  background-color: #222222;\\n}\\n\\nion-img {\\n  height: 30vh;\\n  width: auto;\\n  margin: auto;\\n  display: block;\\n}\\n/*# sourceMappingURL=data:application/json;base64, */\";","export default \"<ion-header>\\n  <ion-toolbar>\\n    <ion-title>Registrazione</ion-title>\\n    <ion-buttons slot=\\\"start\\\">\\n      <ion-back-button></ion-back-button>\\n    </ion-buttons>\\n  </ion-toolbar>\\n</ion-header>\\n\\n<ion-content id=\\\"myContent\\\" class=\\\"ion-padding ion-text-center\\\">\\n\\n    <ion-title class=\\\"ion-text-center\\\">Dati per il Login</ion-title>\\n    <!-- Login -->\\n    <ion-item>\\n      <ion-input placeholder=\\\"Username\\\"  maxlength=\\\"20\\\" [(ngModel)]=\\\"username\\\"></ion-input>\\n    </ion-item>\\n    <ion-item>\\n      <ion-input placeholder=\\\"Password\\\" type=\\\"password\\\"  minlength=\\\"8 