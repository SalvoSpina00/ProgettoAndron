odule],\n})\nexport class RegisterUtentePageRoutingModule {}\n","import { Component, OnInit } from '@angular/core';\nimport { RequestRegisterUtente } from '../../models/request-register-utente';\nimport { ApiService } from '../../services/api-service.service';\nimport { NavController, AlertController } from '@ionic/angular';\n\n@Component({\n  selector: 'app-register-utente',\n  templateUrl: './register-utente.page.html',\n  styleUrls: ['./register-utente.page.scss'],\n})\nexport class RegisterUtentePage implements OnInit {\n  username: string;\n  password: string;\n  checkPassword: string;\n  nome: string;\n  cognome: string;\n  date: Date;\n  cf: string;\n  telefono: string;\n  mail: string;\n  stato: string;\n  regione: string;\n  citta: string;\n  via: string;\n  cap: string;\n  requestRegisterUtente: RequestRegisterUtente;\n  errore: string;\n\n  imgSrc: any;\n  foto: any;\n  mobile:boolean;\n  constructor(private apiService: ApiService, private navCtrl: NavController, private alertController: AlertController) { \n    this.imgSrc= this.apiService.img\n    this.mobile = ! (this.apiService.getPlatform().includes(\"mobileweb\") || this.apiService.getPlatform().includes(\"desktop\"))\n\n  }\n\n  ngOnInit() {}\n\n  register(){\n    this.errore = \"\";\n\n    this.username = this.username ?? \"\"\n    this.password = this.password ?? \"\"\n    this.checkPassword = this.checkPassword ?? \"\"\n    this.nome = this.nome ?? \"\"\n    this.cognome = this.cognome ?? \"\"\n    this.date = this.date ?? null\n    this.cf = this.cf ?? \"\"\n    this.telefono = this.telefono ?? \"\"\n    this.mail = this.mail ?? \"\"\n    this.stato = this.stato ?? \"\"\n    this.regione = this.regione ?? \"\"\n    this.citta = this.citta ?? \"\"\n    this.via = this.via ?? \"\"\n    this.cap = this.cap ?? \"\"\n    \n    if (this.username.trim()===\"\" || this.password.trim()===\"\" || this.checkPassword.trim()===\"\" || this.nome.trim()===\"\" || this.cognome.trim()===\"\" ||\n      this.date == null || this.cf.trim()===\"\" || this.telefono.trim()===\"\" || this.mail.trim()===\"\" || this.stato.trim()===\"\" || this.regione.trim()===\"\"\n      || this.citta.trim()===\"\" || this.via.trim()===\"\" || this.cap.trim()===\"\"){\n      this.errore = \"Compila adeguatamente tutti i campi\"\n      return\n    }\n\n    if (this.password!=this.checkPassword){\n      this.errore = \"Le password non coincidono.\"\n      return\n    }\n\n    if (this.foto == null){\n      this.errore = \"Inserisci una tua foto\"\n      return\n    }\n\n    this.requestRegisterUtente = new RequestRegisterUtente(this.username, this.password, this.checkPassword, this.nome, this.cognome, this.date, this.cf,this.telefono, this.mail, this.stato, this.regione, this.citta, this.via, this.cap, this.foto);\n    this.apiService.registerUtente(this.requestRegisterUtente).subscribe({\n      \n      next:()=>{\n        this.apiService.simplyAlert(\n          ()=>{this.navCtrl.navigateForward(\"/login\")},\n          \"Operazione effettuata\",\n          \"Registrazione avvenuta con successo.\"\n        )\n      },\n\n      error:(error)=>{\n        if(error.status==401){\n          this.errore=\"Username già in uso.\"\n        }\n        else{\n          this.apiService.simplyAlert(\n            ()=>{this.navCtrl.navigateForward(\"/login\")},\n            \"Errore\",\n            \"Si è verificato un errore nella registrazione. Riprova più tardi.\"\n          )\n        }\n      }\n\n    })\n  \n  }\n\n\n\n  chooseImage($event=null){\n    let handler =(imgPath)=>{\n      this.imgSrc = 'data:image/jpeg;base64,' + imgPath;\n      this.foto = imgPath\n    }\n\n    if (this.mobile){\n      this.apiService.chooseImage(handler)\n    }\n    else{\n      this.apiService.getImageBrowser($event,handler)\n    }\n  }\n\n}\n"],"sourceRoot":"webpack:///"}