let ServiziCollettivitaPage = class ServiziCollettivitaPage {
    constructor(api, route, navCtrl) {
        this.api = api;
        this.route = route;
        this.navCtrl = navCtrl;
        this.bisognoSegnalato = false;
        this.imgSrc = this.api.img;
        this.mobile = !(this.api.getPlatform().includes("mobileweb") || this.api.getPlatform().includes("desktop"));
        let handler = () => {
            this.navCtrl.navigateForward("/home");
        };
        Promise.all([this.api.getEts(), this.api.getBisogni("", "", "True")]).then(values => {
            values[0].subscribe({
                next: (resp) => {
                    this.enti = resp["_msg"];
                },
                error: (error) => {
                    this.api.simplyAlert(handler);
                }
            });
            values[1].subscribe({
                next: (resp) => {
                    this.bisogni = resp["_msg"];
                    this.idBisognoEsterno = JSON.parse(this.route.snapshot.paramMap.get("bisogno"));
                    if (this.idBisognoEsterno != null) {
                        this.bisognoSegnalato = true;
                    }
                },
                error: (error) => {
                    this.api.simplyAlert(handler);
                }
            });
        });
    }
    ngOnInit() { }
    sendServizioCollettivita() {
        var _a, _b;
        this.msg = (_a = this.msg) !== null && _a !== void 0 ? _a : "";
        if (this.msg.trim() === "" || this.idEnte == null || this.date == null || (this.bisognoSegnalato && this.idBisogno == null)) {
            this.errore = "Compila adeguatamente tutti i campi";
            return;
        }
        if (this.imgSend == null) {
            this.errore = "Inserisci un allegato";
            return;
        }
        let data = {
            "msg": this.msg,
            "date": this.date,
            "bisognoSegnalato": this.bisognoSegnalato,
            "idBisogno": (_b = this.idBisogno) !== null && _b !== void 0 ? _b : 0,
            "idEnte": this.idEnte,
            "foto": this.imgSend
        };
        let handler = () => {
            this.navCtrl.navigateForward("/home");
        };
        this.api.newServizioCollettivita(data).then(retApiService => {
            retApiService.subscribe({
                next: (result) => {
                    this.api.simplyAlert(handler, "Operazione effettuata", "Grazie per il tuo contributo!");
                },
                error: (err) => {
                    this.api.simplyAlert(handler);
                }
            });
        });
    }
    chooseImage($event = null) {
        let handler = (img) => {
            this.imgSrc = 'data:image/jpeg;base64,' + img;
            this.imgSend = img;
        };
        if (this.mobile) {
            this.api.chooseImage(handler);
        }
        else {
            this.api.getImageBrowser($event, handler);
        }
    }
};
