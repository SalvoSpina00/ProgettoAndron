        function NuovoProdottoPage(apiService, navCtrl) {
          _classCallCheck(this, NuovoProdottoPage);

          this.apiService = apiService;
          this.navCtrl = navCtrl;
        }

        _createClass(NuovoProdottoPage, [{
          key: "ngOnInit",
          value: function ngOnInit() {}
        }, {
          key: "registerProduct",
          value: function registerProduct() {
            var _this = this;

            //* Inizializzazione
            this.bene = new src_app_models_bene__WEBPACK_IMPORTED_MODULE_4__["Bene"]();
            this.bene.nome = this.nome;
            this.bene.marca = this.marca;
            this.bene.descrizione = this.descrizione;
            this.bene.foto = this.foto;
            this.bene.prezzo = this.prezzo;
            this.bene.categoria = this.categoria;
            this.bene.tipo = "bene";
            this.bene.quantita = this.quantita;

            if (this.bene.nome && this.bene.marca && this.bene.descrizione && this.bene.foto && this.bene.prezzo && this.bene.categoria) {
              if (this.bene.nome.trim() == '' || this.bene.marca.trim() == '' || this.bene.descrizione.trim() == '' || this.bene.categoria.trim() == '') {
                this.errore = "Compila adeguatamente tutti i campi";
                return;
              }

              if (isNaN(this.prezzo)) {
                this.errore = "Prezzo dev'essere un numero.";
                return;
              }

              if (isNaN(this.quantita)) {
                this.errore = "Qauntita dev'essere un numero.";
                return;
              }

              this.bene.quantita = Math.trunc(this.bene.quantita);

              if (this.bene.quantita < 0) {
                this.bene.quantita = 0;
              }

              this.bene.prezzo = Math.trunc(this.bene.prezzo);

              if (this.bene.prezzo < 0) {
                this.bene.prezzo = 0;
              }

              this.apiService.newBene(this.bene).then(function (retApiService) {
                retApiService.subscribe(function (response) {
                  console.info(response["_msg"]);

                  _this.navCtrl.navigateForward("/prodotti");
                });
              });
            } else {
              this.errore = "Tutti i campi devono essere compilati.";
            }
          }
        }]);

        return NuovoProdottoPage;
      }();

