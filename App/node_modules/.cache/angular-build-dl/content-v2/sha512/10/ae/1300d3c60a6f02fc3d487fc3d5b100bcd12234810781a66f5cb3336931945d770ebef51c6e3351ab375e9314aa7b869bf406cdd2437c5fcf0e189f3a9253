let ServiziPage = class ServiziPage {
    constructor(api, navCtrl, menuCtrl, popoverCtrl, modalCtrl, toastCtrl) {
        this.api = api;
        this.navCtrl = navCtrl;
        this.menuCtrl = menuCtrl;
        this.popoverCtrl = popoverCtrl;
        this.modalCtrl = modalCtrl;
        this.toastCtrl = toastCtrl;
    }
    ngOnInit() { }
    ionViewWillEnter() {
        this.upperPrice = 500;
        this.lowerPrice = 0;
        this.searchMieiServizi();
    }
    searchMieiServizi() {
        this.api.getMieiServizi().then(retApiService => {
            retApiService.subscribe({
                next: (results) => {
                    this.results = results["_msg"];
                },
                error: (error) => {
                    this.api.simplyAlert();
                }
            });
        });
    }
    delete(idService) {
        return Object(tslib__WEBPACK_IMPORTED_MODULE_0__["__awaiter"])(this, void 0, void 0, function* () {
            this.api.confirmAlert(() => {
                this.api.deleteServizio(idService).then(delRes => {
                    delRes.subscribe({
                        next: () => {
                            this.api.simplyAlert(() => { this.searchMieiServizi(); }, "Operazione effettuata", "Servizio eliminato correttamente");
                        },
                        error: () => {
                            this.api.simplyAlert();
                        }
                    });
                });
            }, "Attenzione", "Vuoi davvero eliminare questo servizio?");
        });
    }
    openNewService() {
        this.navCtrl.navigateForward("/nuovo-servizio");
    }
    popover(type) {
        let msg = type == 0 ?
            "Il servizio da te inserito attualmente è in stato di attesa, verrà valutato se considerarlo valido. Controlla nei prossimi giorni" :
            "Il servizio è attualmente online, chi necessiterà del servizio potrà contattarti!";
        this.api.createPopover(msg);
    }
};
