let RichiestaPage = class RichiestaPage {
    constructor(route, apiService, alertCtrl, router) {
        this.route = route;
        this.apiService = apiService;
        this.alertCtrl = alertCtrl;
        this.router = router;
        this.servizio = null;
    }
    ngOnInit() {
        this.errore = "";
        this.idServizio = JSON.parse(this.route.snapshot.paramMap.get("id"));
        console.log(this.idServizio);
        this.getServizio();
    }
    getServizio() {
        this.apiService.getServizio(this.idServizio).then(retApiService => {
            retApiService.subscribe(results => {
                this.servizio = results["_msg"];
                console.log(this.servizio);
            });
        });
    }
    feedback(id, tipo, nome) {
        this.router.navigate(["/infofeedback", { "id": id, "tipo": tipo, "nome": nome }]);
    }
    richiesta() {
        return Object(tslib__WEBPACK_IMPORTED_MODULE_0__["__awaiter"])(this, void 0, void 0, function* () {
            this.apiService.getBudget().then(retApiService => {
                return retApiService.subscribe((response) => Object(tslib__WEBPACK_IMPORTED_MODULE_0__["__awaiter"])(this, void 0, void 0, function* () {
                    let saldo = response["_msg"].budget;
                    if (saldo < 10) {
                        const alert = yield this.alertCtrl.create({
                            header: "Attenzione",
                            message: "Saldo insufficiente.",
                            buttons: [
                                {
                                    text: 'No',
                                    role: 'cancel',
                                }
                            ]
                        });
                        yield alert.present();
                    }
                    else {
                        if (this.date && this.msg) {
                            if (this.msg.trim() == '') {
                                this.errore = "Compila adeguatamente il campo messaggio";
                                return;
                            }
                            const alert = yield this.alertCtrl.create({
                                header: "Attenzione",
                                message: "Una volta effettuata la richiesta i tuoi token saranno rimossi. Verranno inseriti se la richiesta non andra' a buon fine",
                                buttons: [
                                    {
                                        text: 'No',
                                        role: 'cancel',
                                    },
                                    {
                                        text: 'Si',
                                        handler: () => {
                                            this.apiService.richiesta(this.msg, this.date, this.servizio.id, this.servizio.idUtente).then(retApiService => {
                                                retApiService.subscribe(results => {
                                                    this.alertCtrl.create({
                                                        header: "Richiesta Effettuata Correttamente",
                                                        buttons: [
                                                            {
                                                                text: 'OK',
                                                                role: 'cancel',
                                                            }
                                                        ]
                                                    }).then(alert => alert.present());
                                                });
                                            });
                                        }
                                    }
                                ]
                            });
                            yield alert.present();
                        }
                        else {
                            this.errore = "Tutti i campi devono essere compilati";
                        }
                    }
                }));
            });
        });
    }
};
