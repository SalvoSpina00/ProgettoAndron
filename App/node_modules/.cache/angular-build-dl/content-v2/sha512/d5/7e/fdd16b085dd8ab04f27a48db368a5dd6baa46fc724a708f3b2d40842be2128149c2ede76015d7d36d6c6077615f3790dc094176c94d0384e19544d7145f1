/* harmony import */ var src_app_services_authentication_service__WEBPACK_IMPORTED_MODULE_7__ = __webpack_require__(/*! src/app/services/authentication.service */ "ej43");








let EditProfilePage = class EditProfilePage {
    constructor(apiService, navCtrl, storage, alertController, authenticationService) {
        this.apiService = apiService;
        this.navCtrl = navCtrl;
        this.storage = storage;
        this.alertController = alertController;
        this.authenticationService = authenticationService;
        storage.get('anagraficaJson').then((val) => {
            this.telefono = val["telefono"];
            this.mail = val["mail"];
            this.stato = val["stato"];
            this.regione = val["regione"];
            this.citta = val["citta"];
            this.via = val["via"];
            this.cap = val["cap"];
        });
        this.errore = '';
        this.errorep = '';
    }
    ngOnInit() {
    }
    checkInput() {
        if (!(this.telefono && this.mail && this.stato && this.regione && this.citta && this.via && this.cap)) {
            return false;
        }
        if (this.telefono.trim() == '' || this.mail.trim() == '' || this.stato.trim() == '' || this.regione.trim() == '' || this.citta.trim() == '' || this.via.trim() == '' || this.cap.trim() == '') {
            return false;
        }
        return true;
    }
    editProfile() {
        if (this.checkInput()) {
            this.apiService.updateUtente(this.telefono, this.mail, this.stato, this.regione, this.citta, this.cap, this.via).then(retApiService => {
                retApiService.subscribe((response) => Object(tslib__WEBPACK_IMPORTED_MODULE_0__["__awaiter"])(this, void 0, void 0, function* () {
                    const alert = yield this.alertController.create({
                        header: "Attenzione",
                        message: response["_msg"],
                        buttons: [
                            {
                                text: 'Ok',
                                handler: data => {
                                    this.apiService.getAnagrafica().then(retApiService => {
                                        retApiService.subscribe(response => {
                                            this.authenticationService.setAnagrafica(response["_dati"]);
                                            this.storage.get('anagraficaJson').then((val) => {
                                                this.telefono = val["telefono"];
                                                this.mail = val["mail"];
                                                this.stato = val["stato"];
                                                this.regione = val["regione"];
                                                this.citta = val["citta"];
                                                this.via = val["via"];
                                                this.cap = val["cap"];
                                            });
                                        });
                                    });
                                }
                            }
                        ]
                    });
                    yield alert.present();
                }));
            });
        }
        else {
            this.errore = "Compila adeguatamente i campi";
        }
    }
    cambiaPass() {
        return Object(tslib__WEBPACK_IMPORTED_MODULE_0__["__awaiter"])(this, void 0, void 0, function* () {
            const alert = yield this.alertController.create({
                header: "Attenzione",
                message: "Inserire nuova password e la conferma della password(min 8 caratteri e almeno 1 Maiuscola)",
                inputs: [
                    {
                        name: 'vecchiapassword',
                        type: 'password',
                        placeholder: 'Vecchia password'
                    },
                    {
                        name: 'password',
                        type: 'password',
                        placeholder: 'Nuova password'
                    },
                    {
                        name: 'check',
                        type: 'password',
                        placeholder: 'Conferma password'
                    }
                ],
                buttons: [
                    {
                        text: 'Annulla',
                        role: 'cancel',
                    },
                    {
                        text: 'Ok',
                        handler: data => {
                            if (!(data.password && data.check)) {
                                this.errorep = "Compila tutti i campi";
                                return;
                            }
                            if (data.password.trim() == '' || data.check.trim() == '') {
                                this.errorep = "Password non valida";
                                return;
                            }
                            if (data.password != data.check) {
                                this.errorep = "Le password non coincidono";
                                return;
                            }
                            this.apiService.updatePassword(data.vecchiapassword, data.password, data.check).then(retApiService => {
                                retApiService.subscribe((response) => Object(tslib__WEBPACK_IMPORTED_MODULE_0__["__awaiter"])(this, void 0, void 0, function* () {
                                    const alert = yield this.alertController.create({
                                        header: "Attenzione",
                                        message: response["_msg"],
                                        buttons: [
                                            {
                                                text: 'Ok',
                                                role: 'cancel'
                                            }
                                        ]
                                    });
                                    yield alert.present();
                                }), (errore) => Object(tslib__WEBPACK_IMPORTED_MODULE_0__["__awaiter"])(this, void 0, void 0, function* () {
                                    console.info(errore["error"]["_msg"]);
                                    const alert = yield this.alertController.create({
                                        header: "Attenzione",
                                        message: errore["error"]["_msg"],
                                        buttons: [
                                            {
                                                text: 'Ok',
                                                role: 'cancel'
                                            }
                                        ]
                                    });
                                    yield alert.present();
                                }));
                            });
                        }
                    }
                ]
            });
            yield alert.present();
        });
    }
};
