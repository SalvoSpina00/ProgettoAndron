rt(2, '0'); //January is 0!\n          this.yyyy = this.date.getFullYear();\n          this.today = this.mm + '/' + this.dd + '/' + this.yyyy;\n        },\n        error: (error) =>{\n          this.api.simplyAlert()\n        }\n      })\n    });\n  }\n\n  async evadiOrdine(id:number){\n    this.api.confirmAlert(\n      ()=>{\n        this.api.setCheckOrdine(id).then(retApiService => {\n          retApiService.subscribe({\n            next:()=>{\n              this.api.simplyAlert(()=>{this.searchOrdini()},\"Operazione effettuata\", \"L'ordine è stato evaso, una volta che l'utente lo avrà ricevuto, invierà feedback relativo all'acquisto.\")\n            },\n            error:()=>{\n              this.api.simplyAlert()\n            }\n          })\n        })\n      },\n      \"Attenzione\",\n      \"Vuoi davvero confermare l'evasione dell'ordine? Non sarà possibile annulare l'operazione\",\n    )\n\n  }\n\n\n  searchOrdini(){\n    this.api.getOrdiniImprese().then(retApiService => {\n      retApiService.subscribe({\n        next:(response)=>{\n          this.ordini = response[\"_msg\"];\n        },\n        error:()=>{\n          this.api.simplyAlert()\n        }\n      })\n    });\n  }\n\n}\n"]}