    //#region  //!Alert 
    simplyAlert(operation = () => { }, header = undefined, message = undefined) {
        header = header !== null && header !== void 0 ? header : "Errore";
        message = message !== null && message !== void 0 ? message : "Si è verificato un errore nell'esecuzione dell'operazione richiesta. Riprova più tardi, se il problema dovesse persistere contatta l'amministrazione.";
        this.alertController.create({
            header: header,
            message: message,
            buttons: [
                {
                    text: 'OK',
                    role: 'cancel',
                    handler: operation
                }
            ]
        }).then(alert => alert.present());
    }
    confirmAlert(operation = () => { }, header, message, cancel = () => { }) {
        this.alertController.create({
            header: header,
            message: message,
            buttons: [
                {
                    text: 'No',
                    role: 'cancel',
                    handler: cancel
                },
                { text: 'Si',
                    handler: operation
                }
            ]
        }).then(alert => alert.present());
    }
    //#endregion
    //#region //!Scelta immagini Libreria o fotocamera
    chooseImage(handler, nimage = 1) {
        return Object(tslib__WEBPACK_IMPORTED_MODULE_0__["__awaiter"])(this, void 0, void 0, function* () {
            const actionSheet = yield this.actionSheetController.create({
                header: "Scegli l'immagine",
                buttons: [
                    {
                        text: 'Carica dalla libreria',
                        icon: 'images',
                        handler: () => {
                            this.pickImageFromAlbum(handler, nimage);
                        }
                    },
                    {
                        text: 'Usa la fotocamera',
                        icon: 'camera',
                        handler: () => {
                            this.pickImageFromCamera(handler);
                        }
                    },
                    {
                        text: 'Esci',
                        icon: 'close',
                        role: 'cancel',
                    }
                ]
            });
            yield actionSheet.present();
        });
    }
    pickImageFromAlbum(handler, nimage) {
        let options = {
            maximumImagesCount: nimage,
            outputType: _ionic_native_image_picker_ngx__WEBPACK_IMPORTED_MODULE_5__["OutputType"].DATA_URL,
        };
        this.imagePicker.getPictures(options)
            .then(selectedImg => {
            selectedImg.forEach(handler);
        })
            .catch((error) => {
            this.simplyAlert(() => { }, "Attenzione!", "Non è stato possibile accedere alla libreria, controlla i permessi dell'applicazione.");
        });
    }
    pickImageFromCamera(handler) {
        let options = {
            destinationType: this.camera.DestinationType.DATA_URL,
            mediaType: this.camera.MediaType.PICTURE
        };
        this.camera.getPicture(options)
            .then(handler)
            .catch((error) => {
            if (error == "No Image Selected") {
                return;
            }
            this.simplyAlert(() => { }, "Attenzione!", "Non è stato possibile accedere alla libreria, controlla i permessi dell'applicazione.");
        });
    }
    getCurrentPosition(handler) {
        this.geolocation.getCurrentPosition().then(handler, (error) => {
            this.simplyAlert(() => { }, "Attenzione", "Non è stato possibile recuperare la posizione.");
        });
    }
    getImageBrowser($event, handler) {
        let file = $event.target.files[0];
        let reader = new FileReader();
        reader.onload = (e) => {
            let fileData = e.target.result;
            fileData = fileData.split(",")[1];
            handler(fileData);
        };
        reader.readAsDataURL(file);
    }
    getPlatform() {
        return this.platform.platforms();
    }
    createPopover(msg) {
        this.popover.create({
            component: _pages_popover_popover_page__WEBPACK_IMPORTED_MODULE_8__["PopoverPage"],
            componentProps: { "msg": msg }
        }).then((pop) => {
            pop.present();
        });
    }
};
