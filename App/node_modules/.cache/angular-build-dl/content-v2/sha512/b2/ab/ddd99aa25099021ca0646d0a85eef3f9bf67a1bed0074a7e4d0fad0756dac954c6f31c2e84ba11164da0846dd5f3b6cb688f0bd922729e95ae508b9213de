let ApiService = class ApiService {
    constructor(httpClient, storage, alertController, actionSheetController, imagePicker, camera, geolocation, platform, popover) {
        this.httpClient = httpClient;
        this.storage = storage;
        this.alertController = alertController;
        this.actionSheetController = actionSheetController;
        this.imagePicker = imagePicker;
        this.camera = camera;
        this.geolocation = geolocation;
        this.platform = platform;
        this.popover = popover;
        // API path
        this.path = 'http://192.168.1.70:5000';
        //#endregion
        this.img = "data:image/png;base64,";
        this.user = "data:image/png;base64,";
    }
    //! Update
    updateUtente(telefono, mail, stato, regione, citta, cap, via) {
        return this.storage.get("auth-token").then((a) => {
            this.token = a;
            var httpOptions = {
                headers: new _angular_common_http__WEBPACK_IMPORTED_MODULE_2__["HttpHeaders"]({
                    'Authorization': 'Bearer ' + this.token,
                    'Content-Type': 'application/json'
                })
            };
            this.servizioPath = this.path + "/info";
            return this.httpClient.put(this.servizioPath, JSON.stringify({ "_keyApp": "keyAppSecretPassUtente", "_telefono": telefono, "_mail": mail, "_stato": stato, "_regione": regione, "_citta": citta, "_via": via, "_cap": cap }), httpOptions);
        });
    }
    updatePassword(oldpsw, newpsw) {
        return this.storage.get("auth-token").then((a) => {
            this.token = a;
            var httpOptions = {
                headers: new _angular_common_http__WEBPACK_IMPORTED_MODULE_2__["HttpHeaders"]({
                    'Authorization': 'Bearer ' + this.token,
                    'Content-Type': 'application/json'
                })
            };
            this.servizioPath = this.path + "/info/changePassword";
            return this.httpClient.put(this.servizioPath, JSON.stringify({ "_oldpsw": oldpsw, "_newpsw": newpsw }), httpOptions);
        });
    }
    updateProfilePic(foto) {
        return this.storage.get("auth-token").then((a) => {
            this.token = a;
            var httpOptions = {
                headers: new _angular_common_http__WEBPACK_IMPORTED_MODULE_2__["HttpHeaders"]({
                    'Authorization': 'Bearer ' + this.token,
                    'Content-Type': 'application/json'
                })
            };
            this.servizioPath = this.path + "/info/changeProfilePicture";
            return this.httpClient.put(this.servizioPath, JSON.stringify({ "foto": foto }), httpOptions);
        });
    }
    //!
    getOrdini(psize, p) {
        return this.storage.get("auth-token").then((a) => {
            this.token = a;
            var httpOptions = {
                headers: new _angular_common_http__WEBPACK_IMPORTED_MODULE_2__["HttpHeaders"]({
                    'Authorization': 'Bearer ' + this.token,
                    'Content-Type': 'application/json'
                }),
                params: new _angular_common_http__WEBPACK_IMPORTED_MODULE_2__["HttpParams"]({
                    fromString: "_page=" + p + "&_psize=" + psize
                })
            };
            this.servizioPath = this.path + "/orders/mine";
            return this.httpClient.get(this.servizioPath, httpOptions);
        });
    }
    setCheckOrdine(id) {
        return this.storage.get("auth-token").then((a) => {
            this.token = a;
            var httpOptions = {
                headers: new _angular_common_http__WEBPACK_IMPORTED_MODULE_2__["HttpHeaders"]({
                    'Authorization': 'Bearer ' + this.token,
                    'Content-Type': 'application/json'
                })
            };
