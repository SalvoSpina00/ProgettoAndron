/***/ "c14U":
/*!******************************************!*\
  !*** ./src/app/services/cart.service.ts ***!
  \******************************************/
/*! exports provided: CartService */
/***/ (function(module, __webpack_exports__, __webpack_require__) {

"use strict";
__webpack_require__.r(__webpack_exports__);
/* harmony export (binding) */ __webpack_require__.d(__webpack_exports__, "CartService", function() { return CartService; });
/* harmony import */ var tslib__WEBPACK_IMPORTED_MODULE_0__ = __webpack_require__(/*! tslib */ "mrSG");
/* harmony import */ var _angular_core__WEBPACK_IMPORTED_MODULE_1__ = __webpack_require__(/*! @angular/core */ "fXoL");
/* harmony import */ var _api_service_service__WEBPACK_IMPORTED_MODULE_2__ = __webpack_require__(/*! ./api-service.service */ "ul72");
/* harmony import */ var _models_ordine__WEBPACK_IMPORTED_MODULE_3__ = __webpack_require__(/*! ../models/ordine */ "8HHu");




let CartService = class CartService {
    constructor(apiService) {
        this.apiService = apiService;
        this.cart = [];
        this.cartItemCount = 1;
    }
    getCart() {
        return this.cart;
    }
    getCartItemCount() {
        this.cartItemCount = 0;
        for (let entry of this.cart) {
            this.cartItemCount += entry.quantita;
        }
        return this.cartItemCount;
    }
    addProduct(product) {
        // se ci sono altri problemi controlla il mapping dei dati dopo la rest.
        let added = false;
        for (let p of this.cart) {
            // l'or è necessario perchè quando addProduct viene chiamato da compra add product è di tipo bene mentre quandpo viene chiamato dal cartModale e di tipo Ordine.
            if (p.idProdotto == product.id) {
                p.quantita += 1;
                this.cartItemCount += 1;
                added = true;
            }
        }
        if (!added) {
            // la quantita non è specificata in prodotto è il tuo cartItemCount.
            var prodottoInCart = new _models_ordine__WEBPACK_IMPORTED_MODULE_3__["Ordine"](product.id, product.nodo, product.nome, product.andron, 1);
            this.cart.push(prodottoInCart);
        }
    }
    decreaseProduct(product) {
        for (let p of this.cart) {
            if (p.idProdotto == product.idProdotto) {
                p.quantita -= 1;
                this.cartItemCount = this.cartItemCount - 1;
                if (p.quantita == 0) {
                    this.cart.splice(this.cart.indexOf(p), 1);
                }
            }
        }
    }
    increaseProduct(product) {
        for (let p of this.cart) {
            if (p.idProdotto == product.idProdotto) {
                p.quantita += 1;
                this.cartItemCount = this.cartItemCount + 1;
            }
        }
    }
    resetCart() {
        this.cart = [];
        this.cartItemCount = 1;
    }
    removeProduct(product) {
        this.cartItemCount -= product.quantita;
        for (let p of this.cart) {
            if (p.idProdotto == product.idProdotto) {
                p.quantita = 1;
                this.cart.splice(this.cart.indexOf(p), 1);
            }
        }
    }
};
CartService.ctorParameters = () => [
    { type: _api_service_service__WEBPACK_IMPORTED_MODULE_2__["ApiService"] }
];
CartService = Object(tslib__WEBPACK_IMPORTED_MODULE_0__["__decorate"])([
    Object(_angular_core__WEBPACK_IMPORTED_MODULE_1__["Injectable"])({
        providedIn: 'root'
    }),
    Object(tslib__WEBPACK_IMPORTED_MODULE_0__["__metadata"])("design:paramtypes", [_api_service_service__WEBPACK_IMPORTED_MODULE_2__["ApiService"]])
], CartService);



