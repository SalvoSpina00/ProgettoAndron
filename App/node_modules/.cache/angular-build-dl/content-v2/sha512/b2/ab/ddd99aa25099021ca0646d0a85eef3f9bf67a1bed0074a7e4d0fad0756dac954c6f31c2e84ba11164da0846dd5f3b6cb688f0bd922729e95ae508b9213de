/* harmony import */ var _angular_core__WEBPACK_IMPORTED_MODULE_4__ = __webpack_require__(/*! @angular/core */ "fXoL");
/* harmony import */ var _ionic_angular__WEBPACK_IMPORTED_MODULE_5__ = __webpack_require__(/*! @ionic/angular */ "c7TG");
/* harmony import */ var src_app_services_api_service_service__WEBPACK_IMPORTED_MODULE_6__ = __webpack_require__(/*! src/app/services/api-service.service */ "ul72");







let CartModalPage = class CartModalPage {
    constructor(cartService, modalCtrl, apiService) {
        this.cartService = cartService;
        this.modalCtrl = modalCtrl;
        this.apiService = apiService;
        this.cart = [];
        this.cart = this.cartService.getCart();
    }
    ngOnInit() { }
    decreaseCartItem(product) {
        this.cartService.decreaseProduct(product);
        this.cart = this.cartService.getCart();
    }
    increaseCartItem(product) {
        this.cartService.increaseProduct(product);
        this.cart = this.cartService.getCart();
    }
    removeCartItem(product) {
        this.cartService.removeProduct(product);
        this.cart = this.cartService.getCart();
    }
    getTotal() {
        return this.cart.reduce((i, j) => i + j.prezzo * j.quantita, 0);
    }
    close() {
        this.modalCtrl.dismiss();
    }
    conferma() {
        return Object(tslib__WEBPACK_IMPORTED_MODULE_0__["__awaiter"])(this, void 0, void 0, function* () {
            if (this.cartService.getCart().length == 0) {
                this.apiService.simplyAlert(() => { }, "Attenzione", "Il carrello è vuoto.");
                return;
            }
            this.apiService.confirmAlert(() => { this.checkout(); }, "Attenzione", "Vuoi proseguire con l'ordine?");
        });
    }
    checkout() {
        return Object(tslib__WEBPACK_IMPORTED_MODULE_0__["__awaiter"])(this, void 0, void 0, function* () {
            this.apiService.getBudget().then(retApiService => {
                retApiService.subscribe({
                    next: (response) => {
                        let saldo = response["_msg"].andron;
                        if (saldo < this.getTotal()) {
                            this.apiService.simplyAlert(() => { }, "Attenzione", "Saldo insufficiente per effettuare l'operazione!");
                        }
                        else {
                            this.apiService.newTransazione(this.cart).then(retApiService => {
                                retApiService.subscribe({
                                    next: () => {
                                        this.apiService.simplyAlert(() => {
                                            this.cart = [];
                                            this.cartService.resetCart();
                                            this.modalCtrl.dismiss();
                                        }, "Grazie per il tuo ordine!", "Speriamo che usufrirai ancora dei nostri servizi");
                                    },
                                    error: (errore) => {
                                        console.log(errore);
                                        if (errore.error.status == -1) {
                                            this.apiService.simplyAlert(() => { }, "Attenzione!", errore.error.messaggio);
                                        }
                                        else {
                                            this.apiService.simplyAlert(() => { }, "Attenzione!", "Si è verificato un errore nell'esecuzione, riprova più tardi.");
                                        }
                                    }
                                });
                            });
                        }
                    },
                    error: (error) => {
                        this.apiService.simplyAlert(() => { }, "Attenzione!", "Si è verificato un errore nell'esecuzione, riprova più tardi.");
                    }
                });
            });
        });
    }
};
CartModalPage.ctorParameters = () => [
    { type: _services_cart_service__WEBPACK_IMPORTED_MODULE_3__["CartService"] },
    { type: _ionic_angular__WEBPACK_IMPORTED_MODULE_5__["ModalController"] },
    { type: src_app_services_api_service_service__WEBPACK_IMPORTED_MODULE_6__["ApiService"] }
];
